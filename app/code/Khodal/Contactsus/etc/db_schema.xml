<?xml version="1.0" ?>
<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Setup/Declaration/Schema/etc/schema.xsd">
	<table name="khodal_contactsus" resource="default" engine="innodb" comment="khodal_contactsus Table">
		<column xsi:type="int" name="contactus_id" padding="10" unsigned="true" nullable="false" identity="true" comment="Entity Id"/>
		<constraint xsi:type="primary" referenceId="PRIMARY">
			<column name="contactus_id"/>
		</constraint>
		<column name="name" nullable="true" xsi:type="text" comment="name"/>
		<column name="telephone" nullable="true" xsi:type="varchar" comment="telephone" length="50"/>
		<column name="email" nullable="true" xsi:type="varchar" comment="email" length="255"/>
		<column name="comment" nullable="true" xsi:type="text" comment="comment"/>
	</table>
</schema>
